//#include <piscapisca.h>
#include <16F877A.h>
#device ADC=16

#FUSES NOWDT                    //No Watch Dog Timer
#FUSES NOBROWNOUT               //No brownout reset
#FUSES NOLVP                    //No low voltage prgming, B3(PIC16) or B5(PIC18) used for I/O

#use delay(crystal=20000000)


#define LED1 PIN_B1
#define LED2 PIN_B2
#define LED3 PIN_B3
#define LED4 PIN_B4
#define LED5 PIN_B5
#define LED7 PIN_B7

#define DELAY 100
#define DELAY2 500
#define DELAY3 1000



void main()
{

   //Example blinking LED program
   while(true)
   {
      output_low(LED1);
      delay_ms(DELAY);
      output_high(LED1);
      delay_ms(DELAY);
      
       output_low(LED2);
      delay_ms(DELAY2);
      output_high(LED2);
      delay_ms(DELAY2);
      
   }

}
