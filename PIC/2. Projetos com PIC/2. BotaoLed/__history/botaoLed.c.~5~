#include <botaoLed.h>

void main()
{

   while(TRUE)
   {
      //Ao manter o botâo precionado o led irá acender, se soltar o botao ele apaga
      if(input(pin_a5)==0)
      {
         output_high(pin_b7);
      }
      output_b(0b01111101);
      delay_ms(10);
      output_b(0b00101011);
      delay_ms(10);
   }

}
